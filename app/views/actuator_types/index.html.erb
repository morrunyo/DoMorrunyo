<p id="notice"><%= notice %></p>

<h1>Listing Actuator Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actuator_types.each do |actuator_type| %>
      <tr>
        <td><%= actuator_type.name %></td>
        <td><%= link_to 'Show', actuator_type %></td>
        <td><%= link_to 'Edit', edit_actuator_type_path(actuator_type) %></td>
        <td><%= link_to 'Destroy', actuator_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actuator type', new_actuator_type_path %>
