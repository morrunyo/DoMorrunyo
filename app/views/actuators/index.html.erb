<p id="notice"><%= notice %></p>

<h1>Listing Actuators</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Node</th>
      <th>Actuatortype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actuators.each do |actuator| %>
      <tr>
        <td><%= actuator.name %></td>
        <td><%= actuator.node %></td>
        <td><%= actuator.actuatorType %></td>
        <td><%= link_to 'Show', actuator %></td>
        <td><%= link_to 'Edit', edit_actuator_path(actuator) %></td>
        <td><%= link_to 'Destroy', actuator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actuator', new_actuator_path %>
